  <div id="loading"></div>
  <div class="wrapper-container">
    {% section 'header' %}
    <div class="product-quickview"></div>
    {% include 'breadcrumb' %}
    <main class="main-content" >
      {% if settings.header_layout == "header-vertical-left" %}
      {% include 'header-search-cart' %}
      {% endif %}
      {% if template == "index" %}
      {% if settings.slideshow_enable %}
      <div id="block-top-container" class="">
        <div class="row">
          <div class="col-xs-12 col-sm-12 col-md-12 main-slider-container">
            {% include 'home-revolution' %}
          </div>
        </div>
      </div>
      {% endif %}
      {% endif %}
      <div class="container main_container">
        {{ content_for_layout }}
      </div>
    </main>
    {% section 'footer' %}

    {% include 'quick-view-product' %}

    {% include 'popup-newsletter' %}
    <div id="back-top" style="display: none;"><a class="" href="#top"><i class="cs-font clever-icon-up"></i></a></div>
  </div>
