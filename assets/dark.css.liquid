  color: {{ settings.dark_body_link_color_hover }};
}
.cart-ticky .price-cart-mini,.cart-ticky .text-cart,.cart-ticky #cart-count,.site-nav .level-top > a,.customer-links li a,
#currencies-top .dropdown-toggle,.header-contact .contact-content,.info-contact-top{color: {{ settings.dark_link_color_levertop }};}
@media (max-width: 1024px) {
  .mobile-modern .header,.wrapper-container.offcanvas-push  #header-content,.mobile-minimal .header { background: {{ settings.dark_bg_header_color }} !important; }
  .customer-links li a,#currencies-top label {color: {{ settings.dark_body_color }};}
  .menu-main-home {background: {{ settings.dark_bg_header_color }}; }
  .cusstom-link .customer-links > li > a {color: #333;}
}
@media (max-width: 767px) {
  .flex-control-paging li a {background: #fff;}
  .flex-control-paging li a {background: {{ settings.body_link_color_hover }};}
}